    border-radius: 0em;
    background-color: #333334;
    height: 0.75em;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical
{
    border-left: 0.062em solid #222222;
    border-radius: 0em;
    background-color: #333334;
    height: 0.75em;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

