QTabBar::tab {
    color: #aaaaaa;
    background-color: #333334;
    border-left: none;
    border-right: 0.062em solid #222222;
    border-bottom: 0.125em solid #444444;
    min-width: 0.5em;
    padding: 0.32em;
    margin-bottom: 0.13em;
}

QTabBar::tab:hover {
    color: #333334;
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #888888, stop: 0.89 #888888,
                                stop: 0.9 #666666, stop: 1.0 #666666);
    margin-bottom: 0.13em;
}

QTabBar::tab:selected {
    color: #ffffff;
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #333334, stop: 0.89 #333334,
                                stop: 0.9 #0088cc, stop: 1.0 #0088cc);
    border-top-left-radius: 0em;
    border-top-right-radius: 0em;
    margin-bottom: 0.13em;
}

QTabBar::tab:!selected { /* make non-selected tabs look smaller */
    margin-top: 0em;
}

QTabBar::tab:first:selected { /* the first selected tab has nothing to overlap with on the left */
    margin-left: 0;
}

QTabBar::tab:last:selected { /* the last selected tab has nothing to overlap with on the right */
    margin-right: 0;
}

QTabBar::tab:only-one { /* if there is only one tab, we don't want overlapping margins */
    margin: 0;
}

