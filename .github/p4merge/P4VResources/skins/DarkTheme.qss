/* QCheckBox -------------------------------------------------------------- */

QCheckBox {
    color: #d1d3d6;
}

QCheckBox:focus {
    border: none;
}

QCheckBox QWidget:disabled {
    background-color: #19232d;
    color: #787878;
}

QCheckBox::indicator {
    margin-left: 0.124em;
    width: 0.950em;
    height: 0.950em;
}

QTreeView::indicator:indeterminate,
QCheckBox::indicator:indeterminate {
    image: url(:/checkbox_indeterminate.png);
}

QTreeView::indicator:unchecked,
QCheckBox::indicator:unchecked {
    image: url(:/checkbox_unchecked.png);
}

QTreeView:indicator:unchecked:disabled,
QCheckBox::indicator:unchecked:disabled {
    image: url(:/checkbox_unchecked_disabled.png);
}

QTreeView::indicator:checked,
QCheckBox::indicator:checked {
    image: url(:/checkbox_checked.png);
}

QTreeView::indicator:checked:disabled,
QCheckBox::indicator:checked:disabled{
    image: url(:/checkbox_checked_disabled.png);
}

QTreeView::indicator:indeterminate:disabled,
QCheckBox::indicator:indeterminate:disabled{
    image: url(:/checkbox_indeterminate_disabled.png);
}

